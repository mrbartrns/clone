html(lang='ko')
    head
        title List Page
    body
        h1 List Page
        a(href='/insert') Insert Data
        hr
        table(style='width:100%', border='1')
            tr
                th DELETE
                th EDIT
                th ID
                th NAME
                th Model Number
                th Series
            each item, index in data
                tr 
                    td:a(href='/delete') #{item.id}DELETE
                    td:a(href='/edit') #{item.id}EDIT
                    td #{item.id}
                    td #{item.name}
                    td #{item.modelnumber}
                    td #{item.series}